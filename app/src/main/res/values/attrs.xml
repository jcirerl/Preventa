<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InteractiveLineGraphView">
        <attr name="labelTextSize" format="dimension" />
        <attr name="labelTextColor" format="color" />
        <attr name="labelSeparation" format="dimension" />
        <attr name="axisThickness" format="dimension" />
        <attr name="axisColor" format="color" />
        <attr name="dataThickness" format="dimension" />
        <attr name="dataColor" format="color" />
        <attr name="gridThickness" format="dimension" />
        <attr name="gridColor" format="color" />
    </declare-styleable>
    <declare-styleable name="TimeTable">
        <attr name="calTitlesColor" format="color"/>
        <attr name="calNowTitlesColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="ImageMap">
        <attr name="map" format="string"/>
        <attr name="fitImageToScreen" format="boolean"/>
        <attr name="scaleFromOriginal" format="boolean"/>
        <attr name="maxSizeFactor" format="float"/>
    </declare-styleable>

</resources>
